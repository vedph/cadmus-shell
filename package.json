    "@angular/router": "^14.2.7",
    "@auth0/angular-jwt": "^5.1.0",
    "@datorama/akita": "^7.1.1",
    "@datorama/akita-ngdevtools": "^7.0.0",
    "@myrmidon/auth-jwt-admin": "0.1.1",
    "@myrmidon/auth-jwt-login": "0.1.1",
