    "@myrmidon/ng-tools": "0.1.0",
    "@types/diff-match-patch": "^1.0.32",
    "@types/rangy": "0.0.34",
    "diff-match-patch": "^1.0.5",
    "gravatar": "^1.8.2",
    "ngx-markdown": "^14.0.1",
    "ngx-monaco-editor": "^9.0.0",
    "rangy": "^1.3.0",
